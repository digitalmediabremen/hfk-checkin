{
    "name": "frontend",
    "version": "0.2.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "npm run translate -- --quiet && npm run gen && next build",
        "test-build": "next build",
        "start": "next start -p $PORT",
        "translate": "ts-node --project tsconfig.json -O '{\"module\": \"commonjs\"}' findTranslations.ts -",
        "gen": "run-for-every-file --src 'src/model/api/' --file '*.ts' --not-file '*.validator.ts'  --run 'typescript-json-validator {{src-file}} && replace \"^export [{].*$\" \"\" src/model/api/{{file-name}}.validator.ts'"
    },
    "dependencies": {
        "classnames": "^2.2.6",
        "date-input-polyfill": "^2.14.0",
        "formik": "^2.2.0",
        "get-user-locale": "^1.4.0",
        "json.date-extensions": "^1.2.2",
        "libphonenumber-js": "^1.8.4",
        "next": "^10.0.1",
        "next-pwa": "^3.1.5",
        "normalize.css": "^8.0.1",
        "react": "16.13.1",
        "react-digit-input": "^2.1.0",
        "react-div-100vh": "^0.5.6",
        "react-dom": "16.13.1",
        "react-feather": "^2.0.9",
        "react-hook-form": "^6.15.4",
        "react-page-visibility": "^6.2.0",
        "react-smooth-collapse": "^2.1.0",
        "react-textarea-autosize": "^8.3.1",
        "react-time-input-polyfill": "^1.0.5",
        "react-transition-group": "^4.4.1",
        "react-window": "^1.8.6",
        "react-window-infinite-loader": "^1.0.7",
        "smoothscroll-polyfill": "^0.4.4"
    },
    "devDependencies": {
        "@types/classnames": "^2.2.11",
        "@types/colors": "^1.2.1",
        "@types/node": "^14.11.8",
        "@types/react": "^16.9.52",
        "@types/react-textarea-autosize": "^4.3.5",
        "@types/react-transition-group": "^4.4.0",
        "@types/react-window": "^1.8.2",
        "@types/react-window-infinite-loader": "^1.0.3",
        "@types/smoothscroll-polyfill": "^0.3.1",
        "@types/string-similarity": "^3.0.0",
        "@types/styled-jsx": "^2.2.8",
        "@types/yargs": "^15.0.9",
        "colors": "^1.4.0",
        "replace": "^1.2.0",
        "run-for-every-file": "^1.1.0",
        "string-similarity": "^4.0.2",
        "ts-morph": "^8.1.2",
        "ts-node": "^9.1.1",
        "typescript": "^4.2.2",
        "typescript-json-validator": "^2.4.2",
        "yargs": "^16.1.0"
    }
}
