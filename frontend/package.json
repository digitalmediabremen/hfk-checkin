{
    "name": "frontend",
    "version": "0.2.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "if-env NEXT_PUBLIC_ENVIRONMENT=production && npm run build:production || npm run build:staging",
        "build:production": "npm run translate -- --quiet && npm run gen && next build",
        "build:staging": "npm run gen && next build",
        "postbuild": "rimraf ./.next/**/*.js.map",
        "test-build": "next build",
        "start": "next start -p $PORT",
        "deploy-getin:staging": "git push heroku-getin-frontend-staging HEAD:master",
        "deploy-getin:production": "git push heroku-getin-frontend master:master",
        "deploy-checkin:staging": "git push heroku-checkin-frontend-staging HEAD:master",
        "deploy-checkin:production": "git push heroku-checkin-frontend master:master",
        "translate": "ts-node --project tsconfig.json -O '{\"module\": \"commonjs\"}' findTranslations.ts -",
        "gen": "run-for-every-file --src 'src/model/api/' --file '*.ts' --not-file '*.validator.ts'  --run 'typescript-json-validator {{src-file}} && replace \"^export [{].*$\" \"\" src/model/api/{{file-name}}.validator.ts'"
    },
    "cacheDirectories": [
        ".next/cache",
        "node_modules"
    ],
    "dependencies": {
        "@babel/core": "^7.14.6",
        "@fullcalendar/core": "^5.10.1",
        "@fullcalendar/react": "^5.10.1",
        "@fullcalendar/timegrid": "^5.10.1",
        "@react-hook/resize-observer": "^1.2.2",
        "@sentry/browser": "^6.8.0",
        "@sentry/integrations": "^6.8.0",
        "@sentry/node": "^6.8.0",
        "@sentry/webpack-plugin": "^1.16.0",
        "babel-plugin-superjson-next": "^0.4.2",
        "classnames": "^2.3.1",
        "date-input-polyfill": "^2.14.0",
        "get-user-locale": "^1.4.0",
        "install": "^0.13.0",
        "json.date-extensions": "^1.2.2",
        "libphonenumber-js": "^1.9.21",
        "next": "^10.2.3",
        "next-plugin-modernizr": "0.0.3",
        "next-pwa": "^5.2.23",
        "next-transpile-modules": "^7.0.0",
        "normalize.css": "^8.0.1",
        "npm": "^8.1.2",
        "react": "16.13.1",
        "react-digit-input": "^2.1.0",
        "react-div-100vh": "^0.5.6",
        "react-dom": "16.13.1",
        "react-feather": "^2.0.9",
        "react-hook-form": "^6.15.8",
        "react-page-visibility": "^6.4.0",
        "react-simple-timefield": "^3.2.3",
        "react-smooth-collapse": "^2.1.0",
        "react-textarea-autosize": "^8.3.3",
        "react-transition-group": "^4.4.2",
        "react-window": "^1.8.6",
        "react-window-infinite-loader": "^1.0.7",
        "scrollparent": "^2.0.1",
        "smoothscroll-polyfill": "^0.4.4",
        "superjson": "^1.8.0",
        "use-media": "^1.4.0",
        "workbox-window": "^6.1.5"
    },
    "devDependencies": {
        "@types/classnames": "^2.3.1",
        "@types/colors": "^1.2.1",
        "@types/modernizr": "^3.5.3",
        "@types/node": "^14.17.5",
        "@types/react": "^16.14.11",
        "@types/react-textarea-autosize": "^4.3.6",
        "@types/react-transition-group": "^4.4.2",
        "@types/react-window": "^1.8.4",
        "@types/react-window-infinite-loader": "^1.0.4",
        "@types/scrollparent": "^2.0.0",
        "@types/smoothscroll-polyfill": "^0.3.1",
        "@types/string-similarity": "^3.0.0",
        "@types/styled-jsx": "^2.2.9",
        "@types/yargs": "^15.0.14",
        "colors": "^1.4.0",
        "if-env": "^1.0.4",
        "replace": "^1.2.1",
        "rimraf": "^3.0.2",
        "run-for-every-file": "^1.1.0",
        "string-similarity": "^4.0.4",
        "ts-morph": "^8.2.0",
        "ts-node": "^9.1.1",
        "typescript": "^4.3.5",
        "typescript-json-validator": "^2.4.2",
        "yargs": "^16.2.0"
    }
}
